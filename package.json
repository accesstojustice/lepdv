{
  "name": "lepdv",
  "version": "1.0.0",
  "description": "quotes and buttons and more",
  "main": "webpack.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "hot": "echo \"Warned: we are HOT!...\" && webpack-dev-server  --config webpack.hot.config.js --hot --progress --inline"
  },
  "jest": {
    "testFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-test-renderer",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "automock": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/renemarcelo/lepdv.git"
  },
  "author": "Rene M. Paccha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/renemarcelo/lepdv/issues"
  },
  "homepage": "https://github.com/renemarcelo/lepdv",
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "karma": "^1.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "mocha-loader": "^0.7.1",
    "phantomjs-prebuilt": "^2.1.12",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.17.5",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "jquery": "^3.1.1",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1"
  }
